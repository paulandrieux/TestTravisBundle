{
    "name": "paulandrieux/test-travis-bundle",
    "type": "symfony-bundle",
    "description": "Symfony TestTravisBundle",
    "keywords": ["Test", "travis"],
    "license": "MIT",
    "authors": [
        {
            "name": "Paul Andrieux",
            "email": "paul@appventus.com"
        }
    ],
    "require": {
        "symfony/http-foundation": ">=2.2,<2.3-dev",
        "symfony/form": ">=2.2,<2.3-dev",
        "symfony/validator": ">=2.2,<2.3-dev",
        "symfony/security-bundle": ">=2.2,<2.3-dev",
        "symfony/routing": ">=2.2,<2.3-dev",
        "symfony/config": ">=2.2,<2.3-dev",
        "symfony/console": ">=2.2,<2.3-dev",
        "symfony/twig-bridge": ">=2.2,<2.3-dev",
        "symfony/finder": "2.4.*@dev",
        "twig/twig": ">=1.12,<2.0-dev",
        "doctrine/common": ">=2.2,<3.0"
    },
    "require-dev": {
        "jms/translation-bundle": "*"
    },
    "autoload": {
        "psr-0": { "PaulAndrieux\\TestTravisBundle": "" }
    },
    "target-dir": "PaulAndrieux/TestTravisBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
